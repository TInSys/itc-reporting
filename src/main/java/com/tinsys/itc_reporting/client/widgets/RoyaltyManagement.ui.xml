<!-- <!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent"> -->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:my='urn:import:com.tinsys.itc_reporting.client.widgets.utils'>
	<ui:style src="../mainStyles.css" />
		
	<g:HorizontalPanel width="100%" borderWidth="1">

				<g:VerticalPanel width="100px" styleName="{style.marge}">
					<g:HorizontalPanel width="10%">
						<g:Label text="Company :" styleName="{style.marge}" />
						<g:ValueListBox ui:field="companyListBox"
							styleName="{style.marge}" />
					</g:HorizontalPanel>

					<g:VerticalPanel  width="10%">
						<c:CellTable ui:field="royaltyCellTable" />
						<c:SimplePager ui:field='pager' location='CENTER' />
					</g:VerticalPanel>

					<g:VerticalPanel  width="10%">
						<g:Button ui:field="add" text="Add" styleName="{style.marge}" />
					</g:VerticalPanel>
				</g:VerticalPanel>

				<g:VerticalPanel width="80%" styleName="{style.marge}">
					<my:ShuttleBox ui:field="shuttleBox" />
				</g:VerticalPanel>

	</g:HorizontalPanel>

</ui:UiBinder> 