<!-- <!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent"> -->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:my="urn:import:com.tinsys.itc_reporting.client.widgets.utils"
   >
	<ui:style>
		 .marge {
         margin: 5px;
      }
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel styleName="{style.marge}">
			<my:SortableListBox ui:field="availableItemsListbox" />
			<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" styleName="{style.marge}">
            <g:Button ui:field="addAllButton" text="&gt;&gt;" styleName="{style.marge}"/>
				<g:Button ui:field="addButton" text="&gt;" styleName="{style.marge}"/>
				<g:Button ui:field="removeButton" text="&lt;" styleName="{style.marge}"/>
            <g:Button ui:field="removeAllButton" text="&lt;&lt;" styleName="{style.marge}"/>				
			</g:VerticalPanel>
			<my:SortableListBox ui:field="selectedItemsListbox" />
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 