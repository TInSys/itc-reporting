<!-- <!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent"> -->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="mainStyles.css"/>
	<g:HTMLPanel>
	     <table width="100%">
         <tr>
            <td>
               <g:Label text="iTunes Connect Reporting" styleName="{style.pageHeader}" />
            </td>
         </tr>
      </table>
		<g:DockLayoutPanel unit="EM" height="90%">
			<g:west size="20">
				<g:StackLayoutPanel unit='EM'>
					<g:stack>
						<g:header size='3'>
							Basic Data
						</g:header>
						<g:FlowPanel>
							<g:PushButton ui:field="zoneManagementPushButton"
								text="Manage Zones" styleName="{style.stackButton}" />
							<g:PushButton ui:field="taxManagementPushButton"
                        text="Manage Tax Rates" styleName="{style.stackButton}" />
                     <g:PushButton ui:field="FXRateManagementByZonePushButton"
                        text="Manage FX Rates by Zone" styleName="{style.stackButton}" />
                     <g:PushButton ui:field="FXRateManagementByMonthPushButton"
                        text="Manage FX Rates by Month" styleName="{style.stackButton}" />
                     <g:PushButton ui:field="ApplicationManagementPushButton"
                        text="Manage Applications" styleName="{style.stackButton}" />
						</g:FlowPanel>
					</g:stack>
					<g:stack>
						<g:header size='3'>
							Import Data
						</g:header>
						<g:FlowPanel>
						<g:PushButton ui:field="importFinancialFilesPushButton"
                        text="Import Financial Files" styleName="{style.stackButton}" />
                   </g:FlowPanel>
					</g:stack>
				</g:StackLayoutPanel>
			</g:west>
			<g:center>
				<g:ScrollPanel ui:field="mainPanel">
				</g:ScrollPanel>
			</g:center>
			<g:north size="0">
				<g:HorizontalPanel />
			</g:north>
		</g:DockLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 