<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tinsys.itc_reporting.model.Preferences" table="preferences">
		<id name="id" type="long" >
			<column name="id" />
			<generator class="identity" />
		</id>
 <property generated="never" lazy="false" name="referenceCurrency" type="string">
         <column name="reference_currency" not-null="true" />
 </property>

	</class>
</hibernate-mapping>
